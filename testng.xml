<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="SauceLabTestSuite">
	<listeners>
		<listener
			class-name="com.aventstack.chaintest.plugins.ChainTestListener" />
	</listeners>
	<test thread-count="5" name="LoginTest">
		<classes>
			<class name="com.saucedemo.test.practices.LoginTest" />
		</classes>
	</test> <!-- LoginTest -->
	<test thread-count="5" name="InventoryTest">
		<classes>
			<class name="com.saucedemo.test.practices.InventoryTest" />
		</classes>
	</test> <!-- InventoryTest -->
</suite> <!-- SauceLabTestSuite -->